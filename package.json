{
  "name": "rebell",
  "version": "1.0.1",
  "description": "application for boilerplate typescript",
  "main": "./src/bin/www.ts",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=config/test/tsconfig.test.json mocha -r ts-node/register src/app/**/*.spec.ts",
    "clean": "rimraf -G ./build/* && rimraf -G ./logs/*",
    "remove:docker-image-dev": "docker image rm -f typescript-first-app-dev",
    "remove:docker-image-prod": "docker image rm -f typescript-first-app-prod",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src/**/*.ts",
    "build:pm2": "pm2 kill && yarn clean && yarn lint && yarn test && webpack",
    "start:pm2-dev": "NODE_ENV=development yarn build:pm2",
    "run:pm2-dev": "pm2 delete ./config/pm2/pm2-dev-process.config.js && pm2 start ./config/pm2/pm2-dev-process.config.js",
    "start:pm2-prod": "NODE_ENV=production yarn build:pm2",
    "run:pm2-prod": "pm2 delete ./config/pm2/pm2-prod-process.config.js && pm2 start ./config/pm2/pm2-prod-process.config.js",
    "start:docker": "NODE_ENV=development NODE_DOCKER=true yarn build:docker",
    "build:docker": "yarn clean && yarn lint && yarn test && webpack",
    "start:docker-dev": "NODE_ENV=development NODE_DOCKER=true yarn build:docker-dev",
    "build:docker-dev": "yarn remove:docker-image-dev && yarn clean && yarn lint && yarn test && webpack",
    "run:docker-dev": "docker build -f Dockerfile.dev -t typescript-first-app-dev .",
    "start:docker-prod": "NODE_ENV=production NODE_DOCKER=true yarn build:docker-prod",
    "build:docker-prod": "yarn remove:docker-image-prod && yarn clean && yarn lint && yarn test && webpack",
    "run:docker-prod": "docker build -f Dockerfile.prod -t typescript-first-app-prod .",
    "start:dev": "NODE_ENV=development NODE_LOCAL=true yarn build:dev",
    "build:dev": "yarn clean && yarn lint && webpack",
    "run:dev": "NODE_ENV=development PORT=7030 node build/main.bundle.js",
    "start:prod": "NODE_ENV=production NODE_LOCAL=true yarn build:prod",
    "build:prod": "yarn clean && yarn lint && yarn test && webpack",
    "run:prod": "NODE_ENV=production PORT=7030 node build/main.bundle.js",
    "start": "npx nodemon src/bin/www.ts",
    "dev": "nodemon"
  },
  "keywords": [
    "express",
    "nodejs",
    "swc",
    "chai",
    "mocha",
    "typescript",
    "pm2",
    "webpack",
    "tslog",
    "ajv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhianica/rebell.git"
  },
  "author": "Muchammad Ilham <muchammad.ilham@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.42",
    "@swc/helpers": "^0.4.14",
    "@types/amqplib": "^0.10.1",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^10.0.1",
    "@types/mssql": "^8.1.2",
    "@types/node": "^18.15.10",
    "@types/serve-favicon": "^2.5.3",
    "@types/source-map": "0.5.2",
    "@types/supertest": "^2.0.12",
    "@types/webpack": "^5.28.1",
    "@types/webpack-sources": "^3.2.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.19.0",
    "favicons-webpack-plugin": "^6.0.0",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.8.0",
    "regenerator-runtime": "^0.13.11",
    "source-map": "^0.7.4",
    "supertest": "^6.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "amqplib": "^0.10.3",
    "body-parser": "^1.20.2",
    "dayjs": "^1.11.7",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "favicons": "^7.1.1",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "mssql": "^9.1.1",
    "mysql2": "^3.2.0",
    "pg": "^8.10.0",
    "rebell-core": "^1.0.0",
    "rebell-utils": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^3.1.0",
    "serve-favicon": "^2.5.0",
    "short-crypt": "^3.0.0",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.1.6",
    "tslog": "^4.8.2"
  }
}
